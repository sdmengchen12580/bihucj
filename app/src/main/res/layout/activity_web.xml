<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.aganyun.acode.ui.act.other.WebActivity">

    <!--标题-->
    <RelativeLayout
        android:paddingTop="@dimen/_24dp"
        android:background="@color/theme_color"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_68dp"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/ll_web_back"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <ImageView
                android:layout_width="@dimen/_9dp"
                android:layout_height="@dimen/_15dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="12dp"
                android:src="@drawable/img_back" />
        </LinearLayout>


        <TextView
            android:paddingTop="@dimen/_20dp"
            android:id="@+id/tv_title_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text=""
            android:textColor="@color/white"
            android:textSize="@dimen/_17dp" />


    </RelativeLayout>

    <WebView
        android:id="@+id/webview"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</LinearLayout>
